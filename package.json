{
  "name": "dingtalk-javascript-utility",
  "version": "0.2.6",
  "description": "A modern JavaScript utility library, using in Web and Weex",
  "main": "dist/lib/index.js",
  "scripts": {
    "build": "babel index.js ./package -d dist/lib --copy-files --source-maps && webpack && NODE_ENV=pro webpack",
    "es":"babel index.js ./package --watch -d dist/lib --copy-files --source-maps",
    "umd":"webpack",
    "test": "node ./node_modules/mocha/bin/mocha test/**.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:icepy/dingtalk-javascript-utility.git"
  },
  "author": "icepy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icepy/dingtalk-javascript-utility/issues"
  },
  "homepage": "https://github.com/icepy/dingtalk-javascript-utility#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "chai": "^3.5.0",
    "glob": "^7.1.1",
    "mocha": "^3.3.0",
    "webpack": "^3.8.1"
  }
}
